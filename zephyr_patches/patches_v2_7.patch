+				 enum xec_espi_girq_idx idx,
+				 uint8_t enable)
+{
+	struct espi_xec_config *const cfg = ESPI_XEC_CONFIG(dev);
+
+	if (idx >= max_girq_idx) {
+		return -EINVAL;
