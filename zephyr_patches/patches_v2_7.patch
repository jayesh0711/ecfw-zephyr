+	[ESPI_VWIRE_SIGNAL_HOST_C10]      = {MCHP_MSVW07, ESPI_VWIRE_SRC_ID0,
+					     ESPI_MASTER_TO_SLAVE},
+	/* MSVW08 */
+	[ESPI_VWIRE_SIGNAL_DNX_WARN]      = {MCHP_MSVW08, ESPI_VWIRE_SRC_ID1,
+					     ESPI_MASTER_TO_SLAVE},
