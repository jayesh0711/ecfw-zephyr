+
+			default:
+				data->error_seen = 1;
+				LOG_DBG("%s: %s wait_completion error for data send",
+					__func__, dev->name);
+				break;
+			}
 			return ret;
 		}
 
@@ -505,9 +523,18 @@ static int i2c_xec_poll_read(const struct device *dev, struct i2c_msg msg,
 
